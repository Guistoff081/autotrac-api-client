{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "module": "ESNext",
    "esModuleInterop": true,
    "target": "ESNext",
    "moduleResolution": "Node",
    "sourceMap": true,
    "outDir": "dist",
    "declaration": true,
    "noImplicitAny": true, 
    "types": ["vitest/globals"],
    "skipLibCheck": true
  },
  "include": ["src"],
}
